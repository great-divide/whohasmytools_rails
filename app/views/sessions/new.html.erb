<%= form_for @user, url: {action: "create", controller: "sessions"} do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %> <br>
  <%= f.label :email %>
  <%= f.email_field :email %> <br>
  <%= f.label :password %>
  <%= f.password_field :password %> <br>
  <%= f.submit "Sign me up!" %>
<% end %>


